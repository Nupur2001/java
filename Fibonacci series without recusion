package fibonicci.series;

/**
 *
 * @author NUPUR
 */
public class FibonicciSeries {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int a = 8,c = 0, b = 1;
        System.out.print("First " + a + " terms: ");

        for (int i = 1; i <= a; ++i)
        {
            System.out.print(b + " + ");

            int sum = b + c;
            b = c;
            c = sum;
        }
    }
    
}
