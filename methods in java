public class methods {
    static int logic(int x,int y) {
        int z;
        if (x > y) {
            z = x + y;
        } else{
                z = y - x;
    }
        return z;
    }

    public static void main(String[] args) {
        int a=4,b=9;
        int c;
        c=logic(a,b);
        System.out.println("a"+ " "+"b");
        System.out.println(c);
    }
    }




